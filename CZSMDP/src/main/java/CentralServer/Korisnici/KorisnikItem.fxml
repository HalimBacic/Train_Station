<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="161.0" prefWidth="274.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CentralServer.Korisnici.KorisnikItemControl">
   <children>
      <VBox layoutX="9.0" layoutY="6.0" prefHeight="163.0" prefWidth="264.0">
         <children>
            <TextField fx:id="imeTF" prefHeight="40.0" prefWidth="248.0" promptText="Korisničko ime">
               <padding>
                  <Insets bottom="10.0" />
               </padding>
            </TextField>
            <TextField fx:id="stanicaTF" prefHeight="40.0" prefWidth="248.0" promptText="Stanica">
               <padding>
                  <Insets bottom="10.0" />
               </padding>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </TextField>
            <ButtonBar prefHeight="61.0" prefWidth="224.0">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <buttons>
                  <Button fx:id="brisiBtn" mnemonicParsing="false" onAction="#brisiBtnClick" text="Briši" />
                  <Button fx:id="izvjBtn" mnemonicParsing="false" onAction="#izvjBtnClick" text="Izvještaji" />
               </buttons>
            </ButtonBar>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
